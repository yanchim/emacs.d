;;-*-coding: utf-8;-*-
(define-abbrev-table 'global-abbrev-table
  '(
    ("azdr" "\\([A-Za-z0-9]+\\)" nil :count 0)
    ("bracketr" "\\[\\([^]]+?\\)\\]" nil :count 0)
    ("curlyr" "“\\([^”]+?\\)”" nil :count 0)
    ("dater" "\\([0-9]\\{4\\}-[0-9]\\{2\\}-[0-9]\\{2\\}\\)" nil :count 0)
    ("digitsr" "\\([0-9]+\\)" nil :count 0)
    ("dotr" "\\(.\\)" nil :count 0)
    ("fws" "　" nil :count 0)
    ("strr" "\\([^\"]+?\\)" nil :count 0)
    ("tagr" "\\([</>=\" A-Za-z0-9]+\\)" nil :count 0)
   ))

